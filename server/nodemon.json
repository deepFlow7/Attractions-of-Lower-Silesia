{
  "restartable": "rs",
  "ignore": [
    ".gcloudignore",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node"
  },
  "events": {
    "restart": "echo App restarted"
  },
  "watch": [
    "src/",
    "src/DB/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}